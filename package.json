{
	"tasks": [
		{
		  "label": "stopNode",
		  "type": "shell",
		  "command": "node",
		  "args": ["-e", "process.exit(0)"],
		  "group": {
			"kind": "build",
			"isDefault": true
		  }
		}
	  ],
	"dependencies": {
		"argon2": "^0.41.0",
		"bytebuffer": "^5.0.1",
		"express": "^4.19.2",
		"express-rate-limit": "^6.11.2",
		"jsonwebtoken": "^9.0.2",
		"uuid": "^9.0.1"
	},
	"imports": {
		"#server/*": "./Server/*.js",
		"#user/*": "./User/*.js",
		"#group/*": "./Group/*.js",
		"#world/*": "./World/*.js",
		"#world/objects/*": "./World/Objects/*.js",
		"#updates/*": "./Updates/*.js",
		"#config/*": "./Config/*.js",
		"#masterserver/*": "./MasterServer/*.js"
	},
	"name": "orbweaver",
	"version": "0.0.1",
	"description": "a network overlay for runelite to create custom multiplayer games",
	"main": "index.js",
	"type": "module",
	"keywords": [
		"runescape",
		"old school runescape",
		"runelite"
	],
	"author": "david harris",
	"license": "ISC"
}
